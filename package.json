{
  "name": "magic-8-ball-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --mode production --report",
    "start": "http-server ./dist",
    "dev": "vue-cli-service serve --mode development",
    "lint": "npm run lint:eslint ; npm run lint:stylelint",
    "lint:eslint": "eslint --ext .js,.ts,.json,.html,.vue --ignore-path .eslintignore .",
    "lint:eslint:fix": "eslint --ext .js,.ts,.json,.html,.vue --ignore-path .eslintignore --fix .",
    "lint:fix": "npm run lint:eslint:fix ; npm run lint:stylelint:fix",
    "lint:stylelint": "stylelint '**.css' '**/*.scss' '**/*.vue' --ignore-path .stylelintignore",
    "lint:stylelint:fix": "stylelint '**.css' '**/*.scss' '**/*.vue' --ignore-path .stylelintignore --fix",
    "lint:typescript": "tsc --allowJs --checkJs --noEmit",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "vue": "2.6.10",
    "vue-class-component": "7.0.2",
    "vue-property-decorator": "8.1.0",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@vue/cli-plugin-babel": "3.5.5",
    "@vue/cli-plugin-eslint": "3.5.1",
    "@vue/cli-plugin-typescript": "3.5.3",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "documentation": "9.3.1",
    "eslint": "5.16.0",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-vue": "5.2.2",
    "fibers": "3.1.1",
    "lint-staged": "8.1.5",
    "npm-check-updates": "3.1.4",
    "sass": "1.17.4",
    "sass-loader": "7.1.0",
    "stylelint": "9.10.1",
    "stylelint-color-format": "0.2.0",
    "stylelint-config-recess-order": "2.0.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-strict-scss": "0.5.0",
    "stylelint-scss": "3.5.4",
    "stylelint-webpack-plugin": "0.10.5",
    "ts-jest": "24.0.2",
    "typescript": "3.4.2",
    "vue-cli-plugin-styleguidist": "3.11.4",
    "vue-styleguidist": "3.11.4",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.29.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
